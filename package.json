{
  "name": "@vismalietuva/tslint-formatter-html",
  "version": "1.0.1",
  "description": "HTML formatter for tslint",
  "repository": "ramunsk/tslint-formatter-html",
  "main": "index.js",
  "scripts": {
    "test": "mocha './specs/**/*.specs.js'",
    "test:watch": "npm test -- -w --recursive --watch-extensions js"
  },
  "keywords": [
    "tslint",
    "errors",
    "warnings",
    "check",
    "source",
    "code",
    "typescript",
    "formatter"
  ],
  "author": "ramunsk",
  "dependencies": {
    "vash": "0.12.2"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "mocha": "3.2.0"
  }
}
